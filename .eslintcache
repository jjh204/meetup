mounted = true;\r\n    getEvents().then((events) => {\r\n      if (this.mounted) {\r\n        this.setState({ events, locations: extractLocations(events) });\r\n      }\r\n    });\r\n  }\r\n\r\n  componentWillUnmount() {\r\n    this.mounted = false;\r\n  }\r\n\r\n  state = {\r\n    events: [],\r\n    locations: [],\r\n    number: 32,\r\n    currentLocation: 'all'\r\n  }\r\n\r\n  getData = () => {\r\n    const { locations, events } = this.state;\r\n    const data = locations.map((location) => {\r\n      const number = events.filter((event) => event.location === location).length\r\n      const city = location.split(' ').shift()\r\n      return { city, number };\r\n    })\r\n    return data;\r\n  };\r\n\r\n  updateEvents = (location, eventCount) => {\r\n    getEvents().then((events) => {\r\n      const eventResults = (location === 'all') ?\r\n        events.slice(0, eventCount) :\r\n        events.filter((event) => event.location === location).slice(0, eventCount);\r\n\r\n      this.setState({\r\n        events: eventResults,\r\n        currentLocation: location,\r\n        number: eventCount\r\n      });\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const { locations, number, events, currentLocation } = this.state\r\n    const online = navigator.onLine\r\n    return (\r\n      <div style={{ backgroundImage: `url(${BackGround})`, backgroundRepeat: 'no-repeat', backgroundSize: 'cover', minHeight: '1200px' }}>\r\n        <Navbar collapseOnSelect expand=\"lg\" bg=\"custom\" variant=\"dark\" className=\"fixed-top navbar-main\">\r\n          <Navbar.Brand href=\"https://jjh204.github.io/meetup/\" className=\"navbar-brand\" style={{ color: '#e58d9c' }} >Meet & Social</Navbar.Brand>\r\n          <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\r\n          <Navbar.Collapse id=\"basic-navbar-nav\">\r\n            <Nav className=\"ml-auto\">\r\n              <Nav.Link href=\"https://jjh204.github.io/portfolio-website\" target=\"_blank\" rel=\"noopener\" className=\"navbar-link\" style={{ color: '#e58d9c' }}>Developer Portfolio</Nav.Link>\r\n              <Nav.Link href=\"https://github.com/jjh204\" target=\"_blank\" rel=\"noopener\" className=\"navbar-link\" style={{ color: '#e58d9c' }}>GitHub</Nav.Link>\r\n            </Nav>\r\n          </Navbar.Collapse>\r\n        </Navbar>\r\n        <div className=\"App\">\r\n          <h1>MEET & SOCIAL</h1>\r\n          <h3>Events for the Social Coder</h3>\r\n          <CitySearch locations={locations} number={number} updateEvents={this.updateEvents} />\r\n          <NumberEvents currentLocation={currentLocation} number={number} updateEvents={this.updateEvents} />\r\n          <h3 className=\"dataTitle\">Display of Events per Genre and City:</h3>\r\n          <div className=\"data-vis-wrapper\">\r\n            <EventGenre events={events} />\r\n            <ResponsiveContainer height={400} >\r\n              <ScatterChart margin={{ top: 20, right: 20, bottom: 20, left: 20, }}>\r\n                <CartesianGrid />\r\n                <XAxis type=\"category\" dataKey=\"city\" name=\"City\" />\r\n                <YAxis type=\"number\" dataKey=\"number\" name=\"Number of Events\" allowDecimals={false} />\r\n                <Tooltip cursor={{ strokeDasharray: '3 3' }} />\r\n                <Scatter data={this.getData()} fill=\"#1b253a\" />\r\n              </ScatterChart>\r\n            </ResponsiveContainer>\r\n          </div>\r\n          <EventList events={events} />\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n","/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/serviceWorkerRegistration.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/EventList.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/NumberEvents.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/EventGenre.js",["53"],"import React, { useEffect, useState } from 'react';\nimport { PieChart, Pie, Cell, ResponsiveContainer } from 'recharts';\n\n\nconst EventGenre = ({ events }) => {\n\n  const [data, setData] = useState([]);\n\n  useEffect(() => {\n    setData(() => getData());\n  }, [events]);\n\n  const getData = () => {\n    const genres = ['React', 'JavaScript', 'Node.js', 'jQuery', 'AngularJS', 'Angular'];\n    const data = genres.map((genre) => {\n      const value = events.filter(({ summary }) =>\n        summary.split(' ').includes(genre)\n      ).length;\n      return { name: genre, value };\n    });\n    return data;\n  };\n\n  const COLORS = ['#75364b', '#1b253a', '#366b75', '#6d625f', '#83e3d2', '#364b75'];\n\n  return (\n    <ResponsiveContainer height={400} >\n      <PieChart width={400} height={400}>\n        <Pie\n          data={data}\n          cx={200}\n          cy={200}\n          labelLine={false}\n          outerRadius={80}\n          fill=\"#1b253a\"\n          dataKey=\"value\"\n          label={({ name, percent }) => `${percent > 0 ? name : ''} ${percent > 0 ? (percent * 100).toFixed(0) + '%' : ''}`}\n        >\n          {\n            data.map((entry, index) => <Cell key={`cell-${index}`} fill={COLORS[index % COLORS.length]} name={entry.name} />)\n          }\n        </Pie>\n      </PieChart>\n    </ResponsiveContainer>\n  );\n}\n\nexport default EventGenre;","/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/CitySearch.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/api.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/Alert.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/Event.js",[],"/mnt/c/users/jenni/OneDrive/Documents/GitHub/meetup/src/mock-data.js",[],["54","55"],{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":63,"column":11,"nodeType":"62","messageId":"63","endLine":63,"endColumn":17},{"ruleId":"64","severity":1,"message":"65","line":11,"column":6,"nodeType":"66","endLine":11,"endColumn":14,"suggestions":"67"},{"ruleId":"56","replacedBy":"68"},{"ruleId":"58